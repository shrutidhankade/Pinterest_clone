<%-include("header")%>
    <link rel="stylesheet" href="/stylesheets//signin.css">
    <div class="container">
        <div class="content">
            <!--pinterest logo-->
            <img src="https://i.pinimg.com/originals/d3/d1/75/d3d175e560ae133f1ed5cd4ec173751a.png" alt="pin logo"
                class="img1" />
            <p class="header">Log in to see more</p>

            <% if(error.length> 0){%>
                <p class="error">
                    <%=error%>
                </p>
                <%}%>

           <form action="/signin" method="post">
            <input type="text" name="username" placeholder="username" class="detail" /><br />
            <input type="password" name="password" placeholder="Password" class="detail" />

            <h4><a href="/forget">Forgot your password?</a></h4>


            <button class="btn int">Log in</button>
           </form>
           
            

               <footer>
                <hr />
                <br>
                <p>Not on Pinterest yet? <a href="/signup">signup</a></p>
            </footer>
        </div>
    </div>











    <%-include("footer")%>